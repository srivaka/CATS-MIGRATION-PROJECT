CALL READ_PARM                                                                  
REPL_STR = CAUTDD||CAUTMMM||CAUTYYYY CLSTDD||CLSTMMM||CLSTYYYY                  
INLINE.3 = OVERLAY(REPL_STR,INLINE.3,8)                                         
/* The following logic to force activity on monthly, quarterly or               
   annual batch runs was commented out on Apr. 7, 2005 (CHAPDOU,                
   change tag CL145). */                                                        
/* IF PANSEL < 6 THEN                                                           
      REPL_STR = "N"                                                            
   ELSE                                                                         
      REPL_STR = "Y"                                                            
   INLINE.3 = OVERLAY(REPL_STR,INLINE.3,46)                                     
*/                                                                              
IF LBILNONE = "NONE" THEN                                                       
   REPL_STR = "NONE      NONE     "                                             
ELSE                                                                            
   REPL_STR = LBILDD||LBILMMM||LBILYYYY HBILDD||HBILMMM||HBILYYYY               
IF LPACNONE = "NONE" THEN                                                       
   REPL_STR = REPL_STR||" NONE      NONE     "                                  
ELSE                                                                            
   REPL_STR = REPL_STR LPACDD||LPACMMM||LPACYYYY HPACDD||HPACMMM||,             
   HPACYYYY                                                                     
IF HANNNONE = "NONE" THEN                                                       
   REPL_STR = REPL_STR||" NONE     "                                            
ELSE                                                                            
   REPL_STR = REPL_STR HANNDD||HANNMMM||HANNYYYY                                
IF HCLMNONE = "NONE" THEN                                                       
   REPL_STR = REPL_STR||" NONE     "                                            
ELSE                                                                            
   REPL_STR = REPL_STR HCLMDD||HCLMMMM||HCLMYYYY                                
INLINE.6 = OVERLAY(REPL_STR,INLINE.6,8)                                         
IF HPACNONE ^= "NONE" THEN DO                                                   
   REPL_STR = HPACDD||HPACMMM||HPACYYYY                                         
   INLINE.12 = OVERLAY(REPL_STR,INLINE.12,8)                                    
END                                                                             
IF HANNNONE ^= "NONE" THEN DO                                                   
   REPL_STR = HANNDD||HANNMMM||HANNYYYY                                         
   INLINE.12 = OVERLAY(REPL_STR,INLINE.12,18)                                   
END                                                                             
CALL UPDT_PARM                                                                  
