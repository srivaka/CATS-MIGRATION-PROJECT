PARM_NAME = "CIPRDPUL"                                                          
CALL READ_PARM                                                                  
SELECT                                                                          
     WHEN CAUTMMM = "JAN" THEN REPL_STR = "-01-"                                
     WHEN CAUTMMM = "FEB" THEN REPL_STR = "-02-"                                
     WHEN CAUTMMM = "MAR" THEN REPL_STR = "-03-"                                
     WHEN CAUTMMM = "APR" THEN REPL_STR = "-04-"                                
     WHEN CAUTMMM = "MAY" THEN REPL_STR = "-05-"                                
     WHEN CAUTMMM = "JUN" THEN REPL_STR = "-06-"                                
     WHEN CAUTMMM = "JUL" THEN REPL_STR = "-07-"                                
     WHEN CAUTMMM = "AUG" THEN REPL_STR = "-08-"                                
     WHEN CAUTMMM = "SEP" THEN REPL_STR = "-09-"                                
     WHEN CAUTMMM = "OCT" THEN REPL_STR = "-10-"                                
     WHEN CAUTMMM = "NOV" THEN REPL_STR = "-11-"                                
     WHEN CAUTMMM = "DEC" THEN REPL_STR = "-12-"                                
     OTHERWISE                 REPL_STR = "-01-"                                
END                                                                             
