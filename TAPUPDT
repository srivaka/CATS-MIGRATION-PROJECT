SIGNAL "EXIT"                                                                   
EXIT:                                                                           
/*                                       */                                     
/*  Set PANSEL to exit once updated to   */                                     
/*  stop an endless loop when executing  */                                     
/*  the dialog in batch                  */                                     
/*                                       */                                     
PANSEL="X"                                                                      
"ISPEXEC VPUT (PANSEL)"                                                         
/*                                       */                                     
/* RESUME NORMAL CONSOLE OPERATION       */                                     
/*                                       */                                     
DUMMY = OUTTRAP("OFF")                                                          
IF ERR_COUNT = 0 THEN DO                                                        
   "ISPEXEC SETMSG MSG(TAPM007)"                                                
   "ISPEXEC VPUT (UPDTYPE)"                                                     
END                                                                             
ELSE DO                                                                         
   CALL ERR_TO_SCREEN                                                           
   "ISPEXEC SETMSG MSG(TAPM011)"                                                
END                                                                             
EXIT                                                                            
/*                                        */                                    
/*----------------------------------------*/                                    
/* CALLED PROCEDURES                      */                                    
/*----------------------------------------*/                                    
/*                                        */                                    
/* ALLOCATE AND READ PARAMETER MEMBER     */                                    
/*                                        */                                    
DATE_CONV:                                                                      
/*                                        */                                    
READ_PARM:                                                                      
MAX_RC = 0                                                                      
"ALLOCATE FILE(PARMIN) DATASET('"TAPDSN"("PARM_NAME")') SHR REUSE"              
IF RC ^= 0 THEN DO                                                              
   ERR_COUNT = ERR_COUNT + 1                                                    
   ERR_PARM.ERR_COUNT = PARM_NAME                                               
   ERR_TYPE.ERR_COUNT = "ALLOC"                                                 
   ERR_CODE.ERR_COUNT = RC                                                      
   MAX_RC             = RC                                                      
END                                                                             
/*                                        */                                    
/* DATASET EXISTS, BUT DOES MEMBER?       */                                    
/*                                        */                                    
IF MAX_RC = 0 THEN DO                                                           
   IF SYSDSN("'"TAPDSN"("PARM_NAME")'") ^= "OK" THEN DO                         
      ERR_COUNT = ERR_COUNT + 1                                                 
      ERR_PARM.ERR_COUNT = PARM_NAME                                            
      ERR_TYPE.ERR_COUNT = "SYSDSN"                                             
      ERR_CODE.ERR_COUNT = 20                                                   
      MAX_RC             = 20                                                   
   END                                                                          
END                                                                             
IF MAX_RC = 0 THEN DO                                                           
   "EXECIO * DISKR PARMIN (FINIS STEM INLINE."                                  
   IF RC ^= 0 THEN DO                                                           
      ERR_COUNT = ERR_COUNT + 1                                                 
      ERR_PARM.ERR_COUNT = PARM_NAME                                            
      ERR_TYPE.ERR_COUNT = "READ"                                               
      ERR_CODE.ERR_COUNT = RC                                                   
      MAX_RC             = RC                                                   
   END                                                                          
END                                                                             
RETURN                                                                          
/*                                        */                                    
/* UPDATE AND DEALLOCATE PARAMETER MEMBER */                                    
/*                                        */                                    
UPDT_PARM:                                                                      
IF MAX_RC = 0 THEN DO                                                           
   "EXECIO "INLINE.0" DISKW PARMIN (FINIS STEM INLINE."                         
   IF RC ^= 0 THEN DO                                                           
      ERR_COUNT = ERR_COUNT + 1                                                 
      ERR_PARM.ERR_COUNT = PARM_NAME                                            
      ERR_TYPE.ERR_COUNT = "WRITE"                                              
      ERR_CODE.ERR_COUNT = RC                                                   
      MAX_RC             = RC                                                   
   END                                                                          
END                                                                             
IF MAX_RC = 0 THEN DO                                                           
   "FREE FILE(PARMIN"                                                           
   IF RC ^= 0 THEN DO                                                           
      ERR_COUNT = ERR_COUNT + 1                                                 
      ERR_PARM.ERR_COUNT = PARM_NAME                                            
      ERR_TYPE.ERR_COUNT = "FREE"                                               
      ERR_CODE.ERR_COUNT = RC                                                   
      MAX_RC             = RC                                                   
   END                                                                          
END                                                                             
RETURN                                                                          
/*                                        */                                    
/* COPY REXX TABLES TO ISPF VARS FOR HELP */                                    
/*                                        */                                    
ERR_TO_SCREEN:                                                                  
DO I = 1 TO 20                                                                  
   IF I > ERR_COUNT THEN DO                                                     
      INTERPRET "PARM"I" = ''"                                                  
      INTERPRET "TYPE"I" = ''"                                                  
      INTERPRET "CODE"I" = ''"                                                  
   END                                                                          
   ELSE DO                                                                      
      INTERPRET "PARM"I" = ERR_PARM.I"                                          
      INTERPRET "TYPE"I" = ERR_TYPE.I"                                          
      INTERPRET "CODE"I" = ERR_CODE.I"                                          
   END                                                                          
   "ISPEXEC VPUT (PARM"I" TYPE"I" CODE"I")"                                     
END                                                                             
RETURN                                                                          
